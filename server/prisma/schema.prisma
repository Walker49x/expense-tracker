generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Expense {
  id              String   @id @default(uuid())
  amount          Int
  category        String
  description     String
  date            DateTime
  createdAt       DateTime @default(now())
  idempotencyKey  String   @unique
}
